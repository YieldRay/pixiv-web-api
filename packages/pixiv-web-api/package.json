{
    "name": "@__dirname/pixiv-web-api",
    "version": "0.1.4",
    "type": "module",
    "description": "A thin wrapper of pixiv web api",
    "keywords": [
        "pixiv"
    ],
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "main": "dist/index.js",
    "browser": "./dist/index.umd.js",
    "exports": {
        "import": "./dist/index.js",
        "require": "./dist/index.umd.js"
    },
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "prebuild": "rm -rf dist/*",
        "build": "rollup -c",
        "postbuild": "dts-bundle-generator -o dist/index.d.ts src/index.ts",
        "format": "prettier --write .",
        "docs": "typedoc --plugin ../typedoc-theme-azure/index.js",
        "publish-to-npm": "npm publish --access public"
    },
    "devDependencies": {
        "@rollup/plugin-typescript": "^12.1.4",
        "@types/node": "^24.5.2",
        "rollup": "^4.52.2",
        "tslib": "^2.8.1",
        "typedoc-theme-azure": "workspace:*"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "author": "YieldRay",
    "license": "MIT",
    "repository": {
        "url": "https://github.com/YieldRay/pixiv-web-api",
        "directory": "packages/pixiv-web-api"
    },
    "dependencies": {
        "dts-bundle-generator": "^9.5.1"
    }
}
